{
	"manifest_version": 3,
	"name": "YT Downloader",
	"description": "Downloads a YT video as a mp3 file",
	"version": "1.0",
	"action": {
		"default_popup": "main.html",
		"default_icon": "dl.png"
	},
	"content_scripts": [
		{
			"js": ["scripts/bundle.js"],
			"matches": [
				"*://www.youtube.com/watch*"
			]
		}
	  ]
}